{
  "name": "@webartisanbros/dom-scroll-animator",
  "version": "1.0.1",
  "description": "Wab DOM Scroll Animator library allows to react to scroll events and animate DOM elements based on the scroll position.",
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "validate": "tsc --noEmit",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js",
      "node": "./dist/index.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "wab",
    "dom",
    "scroll",
    "animator",
    "animation",
    "dom"
  ],
  "author": "Leica Florian Robert <florian.leica@webartisanbros.com> (https://webartisanbros.com)",
  "homepage": "https://github.com/Web-Artisan-Bros/wab-dom-scroll-animator",
  "repository": "https://github.com/Web-Artisan-Bros/wab-dom-scroll-animator.git",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "animejs": "^3.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "@types/animejs": "^3.1.7",
    "release-it": "^15.10.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
